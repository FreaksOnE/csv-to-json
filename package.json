{
  "name": "csv-to-json",
  "version": "1.0.0",
  "description": "Converts a csv file to json",
  "author": "freaksone @FreaksOnE",
  "bin": {
    "csv-to-json": "./bin/run"
  },
  "homepage": "https://github.com/FreaksOnE/csv-to-json",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "FreaksOnE/csv-to-json",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "cli-progress": "^3.10.0",
    "csv-parser": "^3.0.0"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/cli-progress": "^3.9.2",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.9.4",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^2",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "csv-to-json",
    "dirname": "csv-to-json",
    "commands": "./dist/commands",
    "defaultCommand": "convert",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {}
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/FreaksOnE/csv-to-json/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
